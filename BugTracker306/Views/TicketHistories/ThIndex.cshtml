@model IEnumerable<BugTracker306.Models.TicketHistory>

@{
    ViewBag.Title = "Index";
}

@if ("1" == ViewBag.ListVolume)
{
    <h2 class="text-center">Ticket History for @ViewBag.TicketTitle</h2>
}
else
{
    <h2 class="text-center">Ticket Histories</h2>
}


@*<p>
    @Html.ActionLink("Create New", "Create")
</p>*@
<table class="table MyDataTables">
    <tr>
        @if ("1" != ViewBag.ListVolume)
        {
            <th>
                @Html.DisplayNameFor(model => model.ticket.Title)
            </th>
        }
        <th>
            @Html.DisplayNameFor(model => model.Property)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.OldValue)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.NewValue)
        </th>
        @*<th>
            @Html.DisplayNameFor(model => model.UserId)
        </th>*@
        <th>
            @Html.DisplayNameFor(model => model.ChangeDt)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        @if ("1" != ViewBag.ListVolume)
        {
            <td>
                @Html.DisplayFor(modelItem => item.ticket.Title)
            </td>
        }
        <td>
            @Html.DisplayFor(modelItem => item.Property)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.OldValue)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.NewValue)
        </td>
        @*<td>
            @Html.DisplayFor(modelItem => item.UserId)
        </td>*@
        <td>
            @Html.DisplayFor(modelItem => item.ChangeDt.Date  )
        </td>
        <td>
            @*@Html.ActionLink("Edit", "Edit", new { id=item.Id }) |*@
            @Html.ActionLink("Details", "ThDetails", new { id=item.Id }) <br />
            @*@Html.ActionLink("Delete", "Delete", new { id=item.Id })*@
        </td>
    </tr>
}

</table>
